{
  "version": "0.0.0-development",
  "name": "winston-tcp",
  "description": "TCP transport for Winston",
  "author": "Ahmad Nassri <ahmad@ahmadnassri.com> (https://www.ahmadnassri.com/)",
  "homepage": "https://github.com/ahmadnassri/winston-tcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadnassri/winston-tcp.git"
  },
  "license": "ISC",
  "main": "build/node4/index.js",
  "keywords": [
    "log",
    "logging",
    "logs",
    "sysadmin",
    "tcp",
    "tools",
    "transport",
    "winston"
  ],
  "engines": {
    "node": ">=4"
  },
  "files": [
    "build",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/ahmadnassri/winston-tcp/issues"
  },
  "scripts": {
    "build": "babel-build-all",
    "lint": "snazzy && echint",
    "pretest": "npm run lint && npm run build",
