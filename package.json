{
  "version": "1.1.2",
  "name": "winston-tcp",
  "description": "TCP transport for Winston",
  "author": "Ahmad Nassri <ahmad@ahmadnassri.com> (https://www.ahmadnassri.com/)",
  "homepage": "https://github.com/ahmadnassri/winston-tcp",
  "repository": "ahmadnassri/winston-tcp",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "log",
    "logging",
    "logs",
    "sysadmin",
    "tcp",
    "tools",
    "transport",
    "winston"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "files": [
    "bin",
    "src",
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/ahmadnassri/winston-tcp/issues"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "test": "babel-node test/* | tap-spec",
    "pretest": "standard && echint --verbose && npm run compile",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul cover --dir coverage tape test/* -- | tap-dot",
    "precoverage": "npm run compile",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "precodeclimate": "npm run coverage",
    "prepublish": "npm run compile"
  },
  "standard": {
    "ignore": [
      "lib/**"
    ]
  },
  "echint": {
    "ignore": [
      "lib/**",
      "coverage/**"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-istanbul": "^0.6.0",
    "babel-preset-es2015": "^6.3.13",
    "codeclimate-test-reporter": "0.1.1",
    "echint": "^1.5.0",
    "mocha": "^2.3.4",
    "should": "^8.1.0",
    "standard": "^6.0.1",
    "tap-dot": "^1.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "debug-log": "^1.0.0",
    "winston": "^2.1.1"
  }
}
