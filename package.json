    "coverage": "tap test --reporter silent --coverage --nyc-arg=--require --nyc-arg=babel-register"
  },
  "standard": {
    "ignore": [
      "build/**"
    ]
  },
  "echint": {
    "ignore": [
      "build/**"
    ]
  },
  "devDependencies": {
    "@ahmadnassri/babel-build-all": "^1.3.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.0.2",
    "babel-register": "^6.18.0",
    "echint": "^3.0.0",
    "snazzy": "^6.0.0",
    "tap": "^10.0.0"
